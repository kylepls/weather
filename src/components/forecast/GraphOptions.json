{
  "backgroundColor": "#35343F",
  "animation": {
    "duration": 1000,
    "startup": true,
    "easing": "inAndOut"
  },
  "chartArea": {
    "width": "99%"
  },
  "annotations": {
    "stemColor": "none",
    "highContrast": false,
    "alwaysOutside": true,
    "textStyle": {
      "color": "white",
      "fontSize": 14
    }
  },
  "tooltip": {
    "trigger": "none"
  },
  "axisTitlesPosition": "none",
  "curveType": "function",
  "vAxis": {
    "gridlines": {
      "color": "transparent"
    },
    "textPosition": "none"
  },
  "hAxis": {
    "gridlines": {
      "count": 2,
      "color": "transparent"
    },
    "textStyle": {
      "color": "white"
    },
    "format": "h a"
  },
  "legend": {
    "position": "none"
  }
}
